{
  "name": "custom-audio-player",
  "version": "1.5.6",
  "license": "MIT",
  "author": "Adalo",
  "main": "index.js",
  "description": "Play any audio file stored in Adalo or via external URL. Great for music, podcasts, etc.",
  "scripts": {
    "start": "adalo dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider && adalo build",
    "login": "adalo login",
    "publish": "adalo publish",
    "publish:local": "REGISTRY_URL=http://localhost:8000 yarn run publish",
    "start:local": "ADALO_DEV_SERVER=http://localhost:3035 yarn start",
    "login:local": "ADALO_BACKEND_SERVER=http://localhost:8084 yarn run login",
    "deploy": "adalo deploy",
    "deploy:local": "REGISTRY_URL=http://localhost:8000 yarn run deploy"
  },
  "peerDependencies": {},
  "adalo": {
    "logo": "./logo.png",
    "displayName": "Custom Audio Player",
    "components": [
      {
        "name": "CustomAudioPlayer",
        "manifest": "./src/components/AudioPlayer/manifest.json"
      }
    ],
    "iosInstallScript": "./scripts/install_ios.sh",
    "androidInstallScript": "./scripts/install_android.sh"
  },
  "devDependencies": {
    "@adalo/cli": "0.0.52",
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-art": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "AdaloHQ/react-native-web"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@protonapp/react-native-material-ui": "^2.0.7",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "moment": "^2.26.0",
    "react-native-swift": "^1.2.3",
    "react-native-track-player": "^4.0.0",
    "react-native-vector-icons": "^10.0.1",
    "uuid": "^9.0.1"
  }
}
